<?xml version="1.0" encoding="utf-8"?>
<layout>

    <data>
        <variable name="SingleActivityViewModel"
                  type="com.example.silvioallgayertrindade.avaliacaodev.viewmodel.SingleActivityViewModel"/>
    </data>

    <android.support.constraint.ConstraintLayout
            xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:tools="http://schemas.android.com/tools"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            tools:context=".presentation.ui.activity.SingleActivity">

        <fragment
                android:id="@+id/navigation_host"
                android:name="androidx.navigation.fragment.NavHostFragment"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:fitsSystemWindows="true"
                app:defaultNavHost="true"
                app:navGraph="@navigation/home_module_navigation" />

    </android.support.constraint.ConstraintLayout>
</layout>